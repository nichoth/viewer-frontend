{
    "extends": [
        "planetary-ssb"
    ],
    "env": {
        "browser": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "max-len": 0,
        "prefer-const": 0,
        "prefer-spread": 0,
        "prefer-rest-params": 0,
        "func-style": 0,
        "indent": [2, 4, { "ignoredNodes": ["TemplateLiteral *"] }]
    }
}

